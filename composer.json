{
    "name": "previewtechs/cpanel-whm-api",
    "description": "Manage WHM and cPanel servers with PHP",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Shaharia Azam",
        "email": "shaharia.azam@gmail.com"
    }],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "guzzlehttp/psr7": "^1.7",
        "php-http/client-implementation": "^1.0",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "PreviewTechs\\cPanelWHM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PreviewTechs\\cPanelWHM\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2.1",
        "phpunit/phpunit": "^5.0.8",
        "php-http/mock-client": "^1.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=psr2 src/",
        "phpunit": "vendor/bin/phpunit"
    }
}